
#include<iostream>
using namespace std;


int main(){

int x, y;

cout << "Enter x coordinate: ";
cin >> x;

cout << "Enter y coordinate: ";
cin >> y;


if (x>0 && y>0){
    cout<<"First quadrant";
}
else if (x>0 && y<0){
    cout<<"Second quadrant";
}

else if(x<0 && y<0){
    cout<<"Third";
}

else if(x==0 && y==0){
  cout<<"origin";
}


else{
    cout<<"Fourth";
}

}
